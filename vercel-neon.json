{
  "version": 2,
  "builds": [
    {
      "src": "index-neon.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index-neon.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index-neon.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "installCommand": "npm install --production=false"
}
